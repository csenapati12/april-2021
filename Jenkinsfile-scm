pipeline{
agent any
 parameters {
    gitParameter branchFilter: 'origin/(.*)', defaultValue: 'main', name: 'BRANCH', type: 'PT_BRANCH_TAG'
    skipDefaultCheckout() 
  }
stages{
stage("prepare"){
steps{
 script{
 //checkout scm
 git branch: "${params.BRANCH}", url: 'https://github.com/csenapati12/april-2021.git'
echo "hello1111"
 }
}
}
 stage("checkout"){
 steps{
//checkout scm
echo "Hello222"
 }
 }
}
}
