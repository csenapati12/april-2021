pipeline{
agent any
 parameters {
    gitParameter branchFilter: 'origin/(.*)', defaultValue: 'master', name: 'BRANCH', type: 'PT_BRANCH'
  }
stages{
stage("prepare"){
steps{
 script{
 git branch: "${params.BRANCH}", url: 'https://github.com/csenapati12/april-2021.git'
echo "hello1111"
 }
}
}
 stage("checkout"){
 steps{
// checkout scm
echo "Hello222"
 }
 }
}
}
